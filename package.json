{
  "name": "teams",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@tanstack/react-query": "^4.24.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.2",
    "styled-components": "^5.3.6",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "vite --host",
    "build": "tsc && vite build",
    "test": "jest",
    "tsc-check": "tsc --noEmit",
    "prettier": "prettier --config ./.prettierrc.json --write './**/*.{js,tsx,ts}'",
    "prettier:check": "prettier --config ./.prettierrc.json --check './**/*.{js,tsx,ts}'",
    "lint:check": "eslint 'src/**/*.{js,tsx,ts}' --max-warnings=0 --cache --cache-location /tmp/eslint-cache.json && npm run tsc-check && npm run prettier:check",
    "lint:fix": "eslint 'src/**/*.{js,tsx,ts}' --fix"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ],
    "preset": "ts-jest",
    "moduleNameMapper": {
      "^@components(.*)$": "<rootDir>/src/components/$1",
      "^@helpers(.*)$": "<rootDir>/src/helpers/$1",
      "^@api(.*)$": "<rootDir>/src/api/$1",
      "^@interfaces(.*)$": "<rootDir>/src/interfaces/$1",
      "^@pages(.*)$": "<rootDir>/src/pages/$1",
      "^@styles(.*)$": "<rootDir>/src/styles/$1",
      "^@tests(.*)$": "<rootDir>/src/tests/$1",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@vitejs/plugin-react": "^2.0.1",
    "babel-jest": "^29.4.3",
    "eslint": "^8.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-no-storage": "^1.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "jest-styled-components": "^7.1.1",
    "prettier": "2.8.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4",
    "vite": "^3.0.7"
  }
}
